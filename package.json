{
  "name": "react-typescript",
  "version": "1.0.0",
  "type": "module",
  "description": "React TypeScript on Replit, using Vite bundler",
  "scripts": {
    "dev": "vite",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir shared/types supabase/functions/trpc/router.ts && cp shared/types/supabase/functions/trpc/router.d.ts shared/types/trpc.types.d.ts",
    "clean:types": "rm -rf shared/types/supabase shared/types/generated",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src",
    "migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "generate": "dotenv -e .env.local -- npx prisma generate",
    "migrate:create": "dotenv -e .env.local -- npx prisma migrate dev --create-only --name hoge",
    "generate:type": "/bin/bash -c '. .env.local && npx supabase gen types typescript --project-id $PROJECT_ID > src/types/database.types.ts'",
    "db:pull": "dotenv -e .env.local -- npx prisma db pull",
    "functions:serve": "npx supabase functions serve",
    "functions:deploy": "/bin/bash -c '. .env.local && npx supabase functions deploy --project-id $PROJECT_ID --no-verify-jwt'",
    "e2e": "npx playwright test --ui --project=chromium --project=auth_chromium --ui-host=0.0.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__user-event": "^4.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.0.6",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "msw": "^2.10.2",
    "playwright-msw": "^3.0.1",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "prisma": "^6.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.2.0",
    "supabase": "^2.22.6",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.1",
    "vitest": "^3.0.6"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.0",
    "@prisma/client": "^6.7.0",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.2",
    "@sentry/react": "^10.20.0",
    "@supabase/supabase-js": "^2.49.1",
    "@tanstack/react-query": "^5.68.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@trpc/tanstack-react-query": "^11.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.475.0",
    "nanoid": "^5.1.5",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  }
}
